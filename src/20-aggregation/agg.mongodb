use("sample_airbnb")
db.listingsAndReviews.find({
    amenities: "Wifi"
}, {
    price: 1,
    amenities:1
})

//aggregation [],[],[]

db.listingsAndReviews.aggregate([
    {$match: {amenities: "Wifi"}}, //find
    {$project: {address: 1}}, //project
    {$group: {_id: "$address.country", count: {$sum: 1}}}
])

